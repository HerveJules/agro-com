    "cookie-parser": "^1.4.4",
    "datauri": "^2.0.0",
    "dotenv": "^7.0.0",
    "email-validator": "^2.0.4",
    "express": "^4.16.4",
    "express-fileupload": "^1.1.4",
    "express-handlebars": "^3.1.0",
    "express-validator": "^5.3.1",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.1",
    "multer-storage-cloudinary": "^2.2.1",
    "passport": "^0.4.0",
