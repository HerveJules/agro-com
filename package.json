    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "eslint-config-airbnb": "^17.1.0",
    "nodemon": "^1.18.11"
  },
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
