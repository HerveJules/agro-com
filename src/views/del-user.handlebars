        <!-- Single pro tab review Start-->
        <div class="single-pro-review-area mt-t-30 mg-b-15">
            <div class="container-fluid">
                <div class="row">
                <span>{{message}}</span>
                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                        <div class="profile-info-inner">
                        <form action="/api/v1/user/erase" method="post">
                            <div class="profile-img" id="user_profile">
                                <img src="{{findOne.image}}" alt="Profile Photo" />
                            </div>
                            
                            <div class="profile-details-hr">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-112">
                                        <input class="form-control" type="text" value="{{findOne.firstname}} {{findOne.lastname}}" placeholder="Full Name..." disabled=""/>
                                    </div>
                                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-112">
                                        <input class="form-control" type="text" value="{{findOne.email}}" placeholder="Email..."  name="email" />
                                    </div>
                                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-112">
                                        <input class="form-control" type="text" value="{{findOne.jobtitle}}" placeholder="Jobtitle..." disabled="" />
                                    </div>
                                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-112">
                                        <input class="form-control" type="text" value="{{findOne.role}}" placeholder="User type..." disabled="" />
                                    </div>&nbsp;
                                
                                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                            
                                                <button class="btn btn-danger ">Delete User</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>