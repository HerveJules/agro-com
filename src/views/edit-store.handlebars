<div class="breadcome-area">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <div class="breadcome-list single-page-breadcome">
                                <div class="row">
                                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                        <div class="breadcome-heading">
                                            <form action="/api/v1/store/modPage" method="post" class="">
                                                <div class="row">
                                                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12"> 
                                                        <input type="text" required="" name="coopName" placeholder="Cooperative name..." class="search-int form-control">
                                                        
                                                    </div>
                                                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12"> 
                                                        <button class="btn btn-default">find</button>
                                                        
                                                    </div>
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                        <ul class="breadcome-menu">
                                            <li><a href="#">Home</a> <span class="bread-slash">/</span>
                                            </li>
                                            <li><span class="bread-blod">Warehouse</span>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Single pro tab review Start-->
        <div class="single-pro-review-area mt-t-30 mg-b-15">
            <div class="container-fluid">
                <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <span>{{message}}</span>
                        <p></p>
                    </div>
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="product-payment-inner-st">
                            <ul id="myTabedu1" class="tab-review-design">
                                <li class="active"><a href="#description">Store Details</a></li>
                            </ul>
                            <div id="myTabContent" class="tab-content custom-product-edit">
                                <div class="product-tab-list tab-pane fade active in" id="description">
                                    <div class="row">
                                    {{#each findAll}}
                                        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
                                            <div class="review-content-section">
                                                <div id="dropzone1" class="pro-ad addcoursepro">

                                                    <form action="/api/v1/store/mod" method="post" enctype="multipart/form-data" id="demo1-upload">
                                                        <div class="row">
                                                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                                <div class="form-group">
                                                                    <select required="" name="id" class="form-control">
                                                                            <option value="none" selected="false" disabled="true">Select cooperative</option>
                                                                            <option selected="true"  value="{{id}}">{{coopName}}</option>
                                                                        </select>
                                                                </div>
                                                                
                                                                <div class="form-group">
                                                                    <select required="" name="productName" class="form-control">
                                                                            <option value="none" selected="true" disabled="true">Select product</option>
                                                                            {{#each Stores}}
                                                                            <option value="{{productName}}">{{productName}}</option>
                                                                            {{/each}}
                                                                        </select>
                                                                </div>
                                                                
                                                                
                                                                <div class="form-group">
                                                                    <select required="" name="quality" class="form-control">
                                                                            <option value="none" selected="true" disabled="true">Select Quality</option>
                                                                            <option value="A">A</option>
                                                                            <option value="B">B</option>
                                                                            <option value="C">C</option>
                                                                            <option value="D">D</option>
                                                                        </select>
                                                                </div>
                                                                <div class="form-group">
                                                                    <input type="number" class="form-control" placeholder="Quantity" name="quantity">
                                                                </div>
                                                                <div class="form-group">
                                                                    <input type="number" class="form-control" placeholder=" Price" name="price">
                                                                </div>
                                                                <div class="form-group">
                                                                    <input type="file" name="uploadfile">
                                                                </div>
                                                                <div class="payment-adress">
                                                                    <button type="submit" class="btn btn-primary waves-effect waves-light">Submit</button>
                                                                </div>
                                                            </div>
                                                            
                                                        
                                                    </form>
                                                    
                                                </div>
                                            </div>
                                        </div>
                                        {{/each}}
                                    </div>
                                </div>
                                
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>