        <div class="contacts-area mg-b-15">
            <div class="container-fluid">
                <div class="row">
                {{#each findAll}}
                    <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12">
                        <div class="hpanel hblue contact-panel contact-panel-cs responsive-mg-b-30">
                        
                            <div class="panel-body custom-panel-jw">
                                
                                <img id="user_profile" src="{{image}}{{profile.image}}" alt="Photo">
                                <p></p>
                                <p><span class="all-pro-ad">Names: </span>{{firstname}} {{user.firstname}} {{lastname}} {{profile.lastname}}</p>
                                <p><span class="all-pro-ad">email: </span>{{email}}</p>
                                <p><span class="all-pro-ad">phone: </span>{{tel}}</p>
                                <p><span class="all-pro-ad">title: </span>{{jobtitle}}</p>
                                <p><span class="all-pro-ad">User-tyep: </span>{{role}}</p>
                            </div>
                            <form action="/api/v1/user/edit/{{id}}" method="get"><button class="btn btn-primary">edit</button></form>
                        </div>
                        &nbsp
                    </div>
                 {{/each}}   
                </div>
            </div>
        </div>